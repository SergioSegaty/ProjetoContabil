<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link rel="icon" href="~Content/flogo.ico" />
</head>
<body>
    <nav class="navbar navbar-expand navbar-light bg-info text-monospace">

        <a class="navbar-brand font-weight-bold" href="#">CONTABIL</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar-collapse collapse" id="navbarNav">
            <ul class="nav navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Index" , "Categoria")">Categorias</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Index" , "Contabilidade")">Contabilidades</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Index" , "Compras")">Compras</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Index" , "CartaoCredito")">Cartão de Crédito</a>
                </li>






            </ul>
        </div>




    </nav>


    <div class="container body-content">
        @RenderBody()

    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>